{
	"name": "example-sdl2-vulkan",
	"dependencies": {
		"bindbc-imgui": "~>1.0.0",
		"bindbc-sdl": "~>1.3.0",
		"erupted": "~>2.1.0",
	},
	"subConfigurations": {
		"bindbc-imgui": "static-SDL2-Vulkan",
	},
	"targetType": "executable",
	"configurations": [
		{
			"name": "dynamic",
			"subConfigurations": {"bindbc-sdl": "dynamic"},
		},
		{
			"name": "static",
			"subConfigurations": {"bindbc-sdl": "static"},
			"libs": ["SDL2"],
		},
	],
	"versions": ["SDL_2018"],
	"libs": ["stdc++", "vulkan"],
}