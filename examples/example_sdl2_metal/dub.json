{
	"name": "example-sdl2-metal",
	"dependencies": {
		"bindbc-imgui": "~>1.0.0",
		"bindbc-sdl": "~>1.3.0",
		"d-metal-binding": "~>1.1.1",
	},
	"subConfigurations": {
		"bindbc-imgui": "static-SDL2-Metal",
	},
	"targetType": "executable",
	"configurations": [
		{
			"name": "dynamic",
			"subConfigurations": {"bindbc-sdl": "dynamic"},
		},
		{
			"name": "static",
			"subConfigurations": {"bindbc-sdl": "static"},
			"libs": ["SDL2"],
		},
	],
	"libs": ["stdc++", "vulkan"],
	"lflags": [
		"-framework","Foundation",
		"-framework","CoreFoundation",
		"-framework","CoreGraphics",
		"-framework","QuartzCore",
		"-framework","Metal",
		"-framework","MetalKit",
		"-framework","Cocoa",
		"-framework","IOKit",
		"-framework","CoreVideo",
	],
	"versions": ["SDL_2018"],
}