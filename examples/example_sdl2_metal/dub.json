{
	"name": "example-sdl2-metal",
	"dependencies": {
		"bindbc-imgui": "~>1.0.0",
		"bindbc-sdl": "~>1.3.0",
		"d-metal-binding": "~>1.0.12",
	},
	"subConfigurations": {
		"bindbc-imgui": "static-SDL2-Metal",
	},
	"targetType": "executable",
	"configurations": [
		{
			"name": "dynamic",
			"subConfigurations": {"bindbc-sdl": "dynamic"},
		},
		{
			"name": "static",
			"subConfigurations": {"bindbc-sdl": "static"},
			"libs": ["SDL2"],
		},
	],
	"versions": ["SDL_2018"],
	"libs": ["vulkan"],
	"lflags": [
		"-framework CoreFoundation",
		"-framework QuartzCore",
		"-framework Metal",
		"-framework MetalKit",
		"-framework Cocoa",
		"-framework IOKit",
		"-framework CoreVideo",
	],
}