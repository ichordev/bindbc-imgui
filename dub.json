{
	"name": "bindbc-imgui",
	"description": "Static bindings to Dear ImGui, compatible with BetterC, @nogc and nothrow.",
	"homepage": "",
	"authors": ["Aya Partridge"],
	"license": "Boost",
	
	"dependencies": {"bindbc-common": "~>0.0.3"},
	"targetType": "staticLibrary",
	"targetPath": "lib",
	"targetName": "BindBC_ImGui",
	
	"sourceFiles-posix": ["$BIND_IMGUI_OBJDIR/*.o"],
	"sourceFiles-windows": ["$BIND_IMGUI_OBJDIR/*.obj"],
	"dflags": ["-extern-std=c++11"],
	"libs": ["stdc++"],
	
	"configurations": [
		{
			"name": "static",
			"versions": [
				"BindImGui_Static"
			]
		},
		{
			"name": "static-SDL2",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0"
			}
		},
		{
			"name": "static-SDL2-Metal",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_Metal"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"d-metal-binding": "~>1.0.12"
			},
			"lflags": [
				"-framework CoreFoundation",
				"-framework QuartzCore",
				"-framework Metal",
				"-framework MetalKit",
				"-framework Cocoa",
				"-framework IOKit",
				"-framework CoreVideo"
			]
		},
		{
			"name": "static-SDL2-OpenGL2",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_OpenGL2"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"bindbc-opengl": "~>1.0.0"
			}
		},
		{
			"name": "static-SDL2-OpenGL3",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_OpenGL3",
				"GL_30"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"bindbc-opengl": "~>1.0.0"
			}
		},
		{
			"name": "static-SDL2-SDLRenderer",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_SDLRenderer"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0"
			}
		},
		{
			"name": "static-SDL2-Vulkan",
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_Vulkan"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"erupted": "~>2.1.0"
			}
		},
		{
			"name": "staticBC",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static"
			]
		},
		{
			"name": "staticBC-SDL2",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0"
			}
		},
		{
			"name": "staticBC-SDL2-Metal",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_Metal"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"d-metal-binding": "~>1.0.12"
			},
			"lflags": [
				"-framework CoreFoundation",
				"-framework QuartzCore",
				"-framework Metal",
				"-framework MetalKit",
				"-framework Cocoa",
				"-framework IOKit",
				"-framework CoreVideo"
			]
		},
		{
			"name": "staticBC-SDL2-OpenGL2",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_OpenGL2"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"bindbc-opengl": "~>1.0.0"
			}
		},
		{
			"name": "staticBC-SDL2-OpenGL3",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_OpenGL3",
				"GL_30"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"bindbc-opengl": "~>1.0.0"
			}
		},
		{
			"name": "staticBC-SDL2-SDLRenderer",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_SDLRenderer"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0"
			}
		},
		{
			"name": "staticBC-SDL2-Vulkan",
			"subConfigurations": {
				"bindbc-common": "yesBC"
			},
			"buildOptions": [
				"betterC"
			],
			"versions": [
				"BindImGui_Static",
				"ImGui_Impl_SDL2",
				"ImGui_Impl_Vulkan"
			],
			"dependencies": {
				"bindbc-sdl": "~>1.3.0",
				"erupted": "~>2.1.0"
			}
		}
	]
}